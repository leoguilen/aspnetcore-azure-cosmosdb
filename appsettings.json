{
  "SwaggerSettings": {
    "JsonRoute": "{documentName}/swagger.json",
    "Description": "This API read and write in book container on Azure Cosmos DB",
    "UIEndpoint": "{version}/swagger.json"
  },
  "CosmosDb": {
    "DatabaseName": "Library",
